grammar regular_definition;

RegDefStart     : [A-Z] ;
RegDefPart      : [a-zA-Z0-9] ;
RegDefId        : RegDefStart RegDefPart*

//EscapeCharacter : '\\' . ;
//ZebuCharacter   : EscapeCharacter | [^\\] ;

TokDot          : '.'  ;
TokComma        : ','  ;
TokColon        : ':'  ;
TokSemiColon    : ';'  ;
TokStar         : '*'  ;
TokPlus         : '+'  ;
TokBar          : '|'  ;
TokCircum       : '^'  ;
TokLParan       : '('  ;
TokRParan       : ')'  ;
TokLBrace       : '{'  ;
TokRBrace       : '}'  ;
TokLBrack       : '['  ;
TokRBrack       : ']'  ;
TokRange        : '-'  ;
TokAt           : '@'  ;

program
  : regular_definitions
  ;

regular_definitions=*NodeList
  : regular_definition=$1
		{
			func pushsym(s *Sym) bool {
				if s.defv {
					fmt.Printf("multiply defined varid %s\n", s)
					return false
				}
				cc.symscope = cc.symscope.add(s)
				s.defv = true
				return true
			}
		}
  | regular_definition=$1 regular_definitions=$2
		{
		}
  ;

regular_definition
  : add
  ;

add
  : expr '+' expr
  ;

oadd
	: 
	| add
	;
